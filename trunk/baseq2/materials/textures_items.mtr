
//
// Adrenaline
//

models/items/adrenal/skin
{
	bumpMap			models/items/adrenal/skin_local.tga
	diffuseMap		models/items/adrenal/skin_d.tga
	{
		specularmap	models/items/adrenal/skin_s.tga
		specularParms	4.0, 2.0
	}
	{
		map		models/items/adrenal/skin_add.tga
		blend		add
		rgb		randomflicker[time * 0.3]
	}
}

